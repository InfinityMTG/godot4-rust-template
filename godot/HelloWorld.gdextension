[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1

[libraries]
linux.debug.x86_64 =     "res://../rust/target/debug/libgd_rs_tutorial.so"
linux.release.x86_64 =   "res://../rust/target/release/libgd_rs_tutorial.so"
windows.debug.x86_64 =   "res://../rust/target/debug/gd_rs_tutorial.dll"
windows.release.x86_64 = "res://../rust/target/release/gd_rs_tutorial.dll"
macos.debug =            "res://../rust/target/debug/libgd_rs_tutorial.dylib"
macos.release =          "res://../rust/target/release/libgd_rs_tutorial.dylib"
macos.debug.arm64 =      "res://../rust/target/debug/libgd_rs_tutorial.dylib"
macos.release.arm64 =    "res://../rust/target/release/libgd_rs_tutorial.dylib"

